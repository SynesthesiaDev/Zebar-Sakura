{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.1/resources/zpack-schema.json",
  "name": "sakura",
  "version": "2026.18.0",
  "description": "Very simple and clean sakura themed bar ",
  "tags": [
    "Sakura",
    "Pink",
    "Cute",
    "Clean"
  ],
  "previewImages": [
    "resources\\preview.png"
  ],
  "repositoryUrl": "https://github.com/SynesthesiaDev/Zebar-Sakura",
  "widgets": [
    {
      "name": "main",
      "htmlPath": "main/dist/index.html",
      "zOrder": "top_most",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": false,
      "transparent": true,
      "includeFiles": [
        "main/dist/**"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "35px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": false,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    }
  ]
}
